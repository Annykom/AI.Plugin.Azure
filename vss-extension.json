{
  "manifestVersion": 1,
  "id": "application-inspector-devops",
  "name": "PT Application Inspector",
  "version": "1.2.0",
  "publisher": "ptaitest",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Tools for code analysis. Includes one task.",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "src"
    }
  ],
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "src"
      }
    }
  ]
}